<div class="row result">
        <div class="col-11 offset-sm-0 offset-md-1 offset-lg-1 resultName">{{placeName}}</div>
        <div class="col-sm-12 col-md-6 col-lg-6 offset-md-1 order-sm-1">
            <iframe width="100%" height="400" frameborder="0" style:"border:0"
            src={{placeURL}} allowfullscreen></iframe>
            
        </div>
        <div class="col-md-5 order-sm-2 resultDetails">Details
            <div class="col-12">Rating:{{placeRating}}</div>
            <div class="col-12">Cost:{{placePricePoint}}</div>
        </div>
        <div class="col-11 offset-sm-0 offset-md-1 offset-lg-1 order-sm-3 resultDetails">{{placeAddress}}</div>
        
        <div class="col-sm-2 offset-5 order-sm-4">
            <form id="ZIPForm" action="resubmit" method="POST" class="row">
                <button class="btn btn-primary" id="pickBtn">Reroll</button>
            </form>
        </div>
</div>